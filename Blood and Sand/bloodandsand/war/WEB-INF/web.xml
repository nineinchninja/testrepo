		<servlet-name>CreateChallengeServlet</servlet-name>
		<url-pattern>/createChallenge</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GladiatorCombat</servlet-name>
		<servlet-class>com.bloodandsand.core.GladiatorCombat</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GladiatorCombat</servlet-name>
		<url-pattern>/cron/gladiatorcombat</url-pattern>
