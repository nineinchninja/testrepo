		<servlet-name>GladiatorTraining</servlet-name>
		<servlet-class>com.bloodandsand.core.GladiatorTraining</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GladiatorTraining</servlet-name>
		<url-pattern>/gladiatortraining</url-pattern>
	</servlet-mapping>	    
